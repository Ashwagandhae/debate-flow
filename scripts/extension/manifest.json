{
	"name": "Debate Flow",
	"version": "0.0.1",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["*://docs.google.com/spreadsheets/*"],
			"css": ["styles.css"],
			"js": ["inject.js"],
			"run_at": "document_end",
			"world": "MAIN"
		},
		{
			"matches": ["*://docs.google.com/spreadsheets/*"],
			"js": ["injectBefore.js"],
			"run_at": "document_start",
			"world": "MAIN"
		}
	],
	"permissions": [],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html"
	}
}
